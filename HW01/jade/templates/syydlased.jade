                    .sub-field.col-sm-6
                        label Sünniaeg
                        input(type="text")
                .row
                    .sub-field.col-sm-6
                        label Kodakontsus
                        select
                            option(default) -- valige kodakontsus --
                            option Eesti
                            option Soome
                            option Läti
                            option Leedu
                            
                    .sub-field.col-sm-6
                        label Riik*
                        select
                            option(default) Eesti
                            option Soome
                            option Läti
                            option Leedu
                .row
                    .sub-field.col-sm-12
                        label Aadress
                        input(type="text")
                .row
                    .sub-field.col-sm-6
                        label E-post
                        input(type="text")
                    .sub-field.col-sm-6
                        label Telefon
                        input(type="text")
